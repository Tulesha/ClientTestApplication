<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:ClientTestApplication.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="using:ClientTestApplication.Views"
        mc:Ignorable="d" d:DesignWidth="1200" d:DesignHeight="650"
        Width="1200" Height="650"
        x:Class="ClientTestApplication.Views.MainWindow"
        Icon="/Assets/Application-logo.ico"
        Title="ClientTestApplication"
        
        
        CanResize="False"
        
        TransparencyLevelHint="AcrylicBlur"
        Background="Transparent"
        
        Content="{Binding Content}">

  <Window.Styles>
    <StyleInclude Source="/Styles/Styles.axaml"/>
  </Window.Styles>
  
</Window>
